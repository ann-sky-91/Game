{
    "compilerOptions": {
        "lib": [
            "ES2021",
            "DOM"
        ],
        "jsx": "react-jsx",
        "module": "ES2022",
        "target": "ES2017",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "typeRoots": [
            "../Modules/node_modules/@types"
        ],
        "baseUrl": ".",
        "paths": {
            "*": [
                "../Modules/node_modules/*"
            ],
            "@/*": [
                "app/*"
            ],
            "sky/*": [
                "../Modules/*"
            ],
            "game/*": [
                "app/*"
            ]
        }
    },
    "include": [
        "deploy.ts",
        "../Modules",
        "app",
        ".sky/"
    ],
    "exclude": [
        "node_modules",
        "../Modules/node_modules"
    ]
}